# 131

과일 = ['사과', '귤', '수박']
for 변수 in 과일:
    print(변수) # 출력: 사과, 귤, 수박

# 132

과일 = ["사과", "귤", "수박"]
for 변수 in 과일:
  print("#####") # 출력: #####, #####, #####

# 133
# for 변수 in ["A", "B", "C"]:
#   print(변수)
변수 = 'A'
print(변수)   # 출력: A
변수 = 'B'
print(변수)   # 출력: B
변수 = 'C'
print(변수)   # 출력: C

# 134
for 변수 in ["A", "B", "C"]:
  print("출력:", 변수)
변수 = 'A'
print(f'출력: {변수}')   # 출력: A
변수 = 'B'
print(f'출력: {변수}')   # 출력: B
변수 = 'C'
print(f'출력: {변수}')   # 출력: C

# 135
# for 변수 in ['A', 'B', 'C']:
#     b = 변수.lower()
#     print(f'변환: {b}')
변수 = 'A'
b = 변수.lower()
print(f'변환: {변수}')  # 출력: 변환: a
변수 = 'B'
b = 변수.lower()
print(f'변환: {변수}')  # 출력: 변환: b
변수 = 'C'
b = 변수.lower()
print(f'변환: {변수}')  # 출력: 변환: c

# 136

for 변수 in range(10,31,10):
    print(변수) # 출력: 10, 20, 30,

# 137

for 변수 in [10, 20, 30]:
    print(변수)   # 출력: 10, 20, 30

# 138
for i in range(10,31,10):
    print(i)            # 출력: 10, 20, 30
    print('-------')    # 출력: -------, -------, -------

# 139
print('++++')       # 출력: ++++
for i in range(10,31,10):
    print(i)        # 출력: 10, 20, 30

# 140

for _ in range(4):
    print("-------")    # 출력: -------, -------, -------, -------
