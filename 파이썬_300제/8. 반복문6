# 181
apart = [['101호', '102호'], ['201호', '202호'], ['301호', '302호']]
print(apart)    # 출력: [['101호', '102호'], ['201호', '202호'], ['301호', '302호']]

# 182
stock = [['시가', 100, 200, 300], ['종가', 80, 210, 330]]
print(stock)    # 출력: [['시가', 100, 200, 300], ['종가', 80, 210, 330]]

# 183
stock = {'시가': [100, 200, 300], '종가': [80, 210, 330]}
print(stock)    # 출력: {'시가': [100, 200, 300], '종가': [80, 210, 330]}

# 184
stock = {'10/10': [80, 110, 70, 90], '10/11': [210, 230, 190, 200]}
print(stock)    # 출력: {'10/10': [80, 110, 70, 90], '10/11': [210, 230, 190, 200]}

# 185
apart = [[101, 102], [201, 202], [301, 302]]
for convert in apart:
    for c in convert:
        print(c, '호')  # 출력: 101 호
                        #       102 호
                        #       201 호
                        #       202 호
                        #       301 호
                        #       302 호
# 186
for i in range(len(apart)-1,-1,-1):
    for convert in apart[i]:
        print(convert, '호')    # 출력: 301 호
                                #       302 호
                                #       201 호
                                #       202 호
                                #       101 호
                                #       102 호

# 187
for apt in apart[::-1]:
    for a in apt[::-1]:
        print(f'{a} 호')     # 출력: 302 호
                            #       301 호
                            #       202 호
                            #       201 호
                            #       102 호
                            #       101 호

# 188
for apt in apart:
    for a in apt:
        print('%d 호' % (a)) # 출력: 101 호, 102 호, 201 호, 202 호, 301 호, 302호
        print('-----')  # 출력: -----, -----, -----, -----, -----, -----

# 189
for apt in apart:
    for a in apt:
        print('{} 호'.format(a)) # 출력: 101 호, 102 호, 201 호, 202 호, 301 호, 302 호
    print('-----')  # 출력: -----, -----, -----

# 190
for i in range(len(apart)):
    for j in range(len(apart[i])):
        print(f'{apart[i][j]} 호') # 출력: 101 호, 102 호, 201 호, 202 호, 301 호, 302호
print('-----') # 출력: -----
