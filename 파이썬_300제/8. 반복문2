# 141
LIST = [100, 200, 300]
for L in LIST:
    print(L + 10)       # 출력: 110
                        #       210
                        #       310

# 142
LIST = ['김밥', '라면', '튀김']
for L in LIST:
    print('오늘의 메뉴:', L)     # 출력: 오늘의 메뉴: 김밥
                                #       오늘의 메뉴: 라면
                                #       오늘의 메뉴: 튀김

# 143
LIST = ['SK하이닉스', '삼성전자', 'LG전자']
for L in LIST:
    print(len(L))                           # 출력: 6
                                            #       4
                                            #       4

# 144
LIST = ['dog', 'cat', 'parrot']
for L in LIST:
    print(L, len(L))            # 출력: dog 3
                                #       cat 3
                                #       parrot 6

# 145
LIST = ['dog', 'cat', 'parrot']
for L in LIST:
    print(L[0])                 # 출력: d
                                #       c
                                #       p

# 146
LIST = [1, 2, 3]
for L in LIST:
    print(f'3 x {L}')   # 출력: 3 x 1
                        #       3 x 2
                        #       3 x 3

# 147
for L in LIST:
    print(f'3 x {L} = {3*L}')   # 출력: 3 x 1 = 3
                                #       3 x 2 = 6
                                #       3 x 3 = 9

# 148
LIST = ['가', '나', '다', '라']
for L in LIST[1:]:
    print(L)                    # 출력: 나
                                #       다
                                #       라

# 149
for L in LIST[::2]:
    print(L)        # 출력: 가
                    #       다

# 150
for L in LIST[::-1]:
    print(L)            # 출력: 라
                        #       다
                        #       나
                        #       가
