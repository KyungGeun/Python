# 151
LIST = [3, -20, -3, 44]
for L in LIST:
    if L < 0:
        print(L)        # 출력: -20
                        #       -3

# 152
LIST = [3, 100, 23, 44]
for L in LIST:
    if L % 3 == 0:
        print(L)        # 출력: 3

# 153
LIST = [13, 21, 12, 14, 30, 18]
for L in LIST:
    if L < 20 and L % 3 == 0:
        print(L)                # 출력: 12
                                #       18

# 154
LIST = ["I", "study", "python", "language", "!"]
for L in LIST:
    if len(L) >= 3:
        print(L)                    # 출력: study
                                    #       python
                                    #       language

# 155
LIST = ['A', 'b', 'c', 'D']
for L in LIST:
    if L.isupper():
        print(L)            # 출력: A
                            #       D

# 156
for L in LIST:
    if L.islower():
        print(L)            # 출력: b
                            #       c

# 157
LIST = ['dog', 'cat', 'parrot']
for L in LIST:
#   print(f'{L.capitalize()}')
    print(L[0].upper()+L[1:])   # 출력: Dog
                                #       Cat
                                #       Parrot

# 158
LIST = ['hello.py', 'ex01.py', 'intro.hwp']
for L in LIST:
    print(L.split('.')[0])      # 출력: hello
                                #       ex01
                                #       intro

# 159
LIST = ['intra.h', 'intra.c', 'define.h', 'run.py']
for L in LIST:
    if L.split('.')[1] == 'h':
        print(L)                    # 출력: intra.h
                                    #       define.h

# 160
for L in LIST:
    if L.split('.')[1] != 'py':
        print(L)                    # 출력: intra.h
                                    #       intra.c
                                    #       define.h
