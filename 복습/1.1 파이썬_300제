# 011
삼성전자 = 50000
총_평가금액 = 삼성전자 * 10
print(총_평가금액) # 출력: 500000

# 012
시가총액 = 298000000000000
현재가 = 50000
PER = 15.79
print(시가총액, type(시가총액)) # 출력: 298000000000000 <class 'int'>
print(현재가, type(현재가)) # 출력: 50000 <class 'int'>
print(PER, type(PER)) # 출력: 15.79 <class 'float'>

# 013
s = 'hello'
t = 'python'
print(s+ '!', t) # 출력: hello! python
# print(f'{s}! {t}')

# 014
print(2 + 2 * 3) # 출력 8

# 015
a = '132'
print(type(a)) # 출력 <class 'str'>

# 016
num_str = '720'
num_str = int(num_str)
print(num_str, type(num_str)) # 출력 720 <class 'int'>

# 017
num = 100
num = str(num)
print(num, type(num)) # 출력: 100 <class 'str'>

# 018
float_str = '15.79'
float_str = float(float_str)
print(float_str, type(float_str)) # 출력: 15.79 <class 'float'>

# 019
year = '2020'
year = int(year)
print(year, type(year)) # 출력: 2020 <class 'int'>

# 020
month = 48584
total = month * 36
print(total) # 출력 1749024
